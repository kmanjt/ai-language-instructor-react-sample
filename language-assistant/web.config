<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="server.js" verb="*" modules="iisnode"/>
    </handlers>
    <rewrite>
      <rules>
        <rule name="NodeInterceptor" patternSyntax="Wildcard">
          <match url="*"/>
          <action type="Rewrite" url=".next/standalone/server.js"/>
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
